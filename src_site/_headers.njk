  Accept-CH: Downlink,RTT,Device-Memory,Save-Data,DPR,Width
  Content-Security-Policy: default-src 'none'; style-src: 'self'; script-src 'self' https://www.youtube.com https://static.cloudflareinsights.com; img-src 'self' https://webmention.io https://avatars.webmention.io *.amazonaws.com data: https://i.ytimg.com; frame-src 'self' https://www.youtube-nocookie.com https://www.youtube.com; connect-src https://cloudflareinsights.com/cdn-cgi/rum; object-src 'none'; font-src 'self'; frame-ancestors 'self'; form-action 'self'; base-uri 'none';

/static/*
  X-Robots-Tag: nosnippet

/dist/*
  X-Robots-Tag: noindex
