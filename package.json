{
  "name": "dgrammatiko.online",
  "private": true,
  "version": "0.0.1",
  "description": "Personal site",
  "main": "build.js",
  "scripts": {
    "copy-assets": "node ./build-scripts/copy-assets.js",
    "deploy-patch": "npm version patch --force -m \"version %s\"",
    "postversion": "git push && git push --tags",
    "deploy": "node ./build-scripts/deploy.js",
    "build": "node ./build-scripts/clean.js && node ./build-scripts/copy-assets.js && node ./build-scripts/copy-static.js && node ./build-scripts/glob/buildAllCss.js && node ./build-scripts/buildjs.js && node ./build-scripts/responsive-images.js && npx eleventy",
    "dev": "node ./build-scripts/clean.js && node ./build-scripts/copy-assets.js && node ./build-scripts/copy-static.js && node ./build-scripts/glob/buildAllCss.js && node ./build-scripts/buildjs.js && node ./build-scripts/responsive-images.js && npx eleventy --serve"
  },
  "author": "dgrammatiko",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "0.10.0",
    "@11ty/eleventy-plugin-rss": "1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.0.1",
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@gerhobbelt/markdown-it-responsive": "latest",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.0",
    "@tbranyen/jsdom": "^13.0.0",
    "cssnano": "4.1.10",
    "fs-extra": "8.1.0",
    "gh-pages": "2.2.0",
    "glob": "7.1.6",
    "html-minifier": "4.0.0",
    "http-server": "0.12.1",
    "image-size": "^0.8.3",
    "lodash": "4.17.15",
    "luxon": "latest",
    "markdown-it-anchor": "5.2.5",
    "mkdirp": "1.0.3",
    "path": "0.12.7",
    "postcss": "7.0.26",
    "postcss-custom-media": "7.0.8",
    "postcss-custom-selectors": "5.1.2",
    "postcss-discard-comments": "4.0.2",
    "postcss-easy-import": "3.0.0",
    "postcss-import": "12.0.1",
    "postcss-mixins": "6.2.3",
    "postcss-nested": "4.2.1",
    "postcss-preset-env": "6.7.0",
    "rename": "1.0.4",
    "require-dir": "latest",
    "rmrf": "2.0.2",
    "rollup": "1.31.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-terser": "5.2.0",
    "sharp": "0.24.0",
    "terser": "4.6.3"
  },
  "dependencies": {
    "ce-theme-switcher": "0.0.3",
    "intersection-observer": "0.7.0"
  }
}
